{
  "name": "o2-nutrition-backend",
  "version": "1.0.0",
  "description": "O2 Nutrition Store Backend with Payload CMS",
  "main": "dist/server.js",
  "scripts": {
    "build": "cross-env NODE_OPTIONS=--no-deprecation next build",
    "dev": "cross-env NODE_OPTIONS=--no-deprecation next dev",
    "dev:prod": "cross-env NODE_OPTIONS=--no-deprecation rm -rf .next && npm run build && npm run start",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start"
  },
  "dependencies": {
    "@payloadcms/db-mongodb": "^3.0.0-beta.111",
    "@payloadcms/next": "^3.0.0-beta.111",
    "@payloadcms/richtext-lexical": "^3.0.0-beta.111",
    "@payloadcms/ui": "^3.0.0-beta.111",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/xlsx": "^0.0.35",
    "cross-env": "^7.0.3",
    "graphql": "^16.8.1",
    "jsonwebtoken": "^9.0.2",
    "next": "15.0.2",
    "payload": "^3.0.0-beta.111",
    "react": "^19.0.0-rc-02c0e824-20241028",
    "react-compiler-runtime": "^19.1.0-rc.3",
    "react-dom": "^19.0.0-rc-02c0e824-20241028",
    "sharp": "0.32.6",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@types/node": "^20.12.7",
    "@types/react": "npm:types-react@19.0.0-rc.0",
    "@types/react-dom": "npm:types-react-dom@19.0.0-rc.0",
    "typescript": "5.5.4"
  },
  "peerDependencies": {
    "@types/react": "npm:types-react@19.0.0-rc.0",
    "@types/react-dom": "npm:types-react-dom@19.0.0-rc.0"
  },
  "overrides": {
    "@types/react": "npm:types-react@19.0.0-rc.0",
    "@types/react-dom": "npm:types-react-dom@19.0.0-rc.0"
  }
}
